!!!
%html
  %head
    %title
      <%= app_name.titlecase %>

    = csrf_meta_tags

    = javascript_pack_tag 'application'
    = stylesheet_pack_tag 'application'

  %body{ data: {env: Rails.env} }
    = query_diet_widget(bad_count: 15) if Rails.env.development?

    .container
      = render 'layouts/menu_bar'
      = render 'layouts/flashes'

      .content
        = yield
